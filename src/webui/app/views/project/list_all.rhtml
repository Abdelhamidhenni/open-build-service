<h2>All Projects</h2>

<p><%= link_to '[Create New Project]', :action => :new %></p>

<table>
  <tr><th>Name</th>
  <% for project in @projects %>
  <tr><td>
    <%= link_to project.name, :action => :show, :project => project %>
  </td>
  </tr>
  <% end %>
</table>

<% if @project_pages.page_count > 1 %>
Pages <%= pagination_links( @project_pages ) %>
<% end %>
