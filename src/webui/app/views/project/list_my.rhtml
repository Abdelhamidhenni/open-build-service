<h2>All Projects</h2>

<% for project in @projects -%>
<%= link_to project, :action => :show, :name => project %><br/>
<% end -%>
<br/>
<h2>My Watched Projects</h2>
<% if @watchlist -%>
<% @watchlist.each_project do |project| -%>
<%= link_to project, :action => :show, :name => project %><br/>
<% end -%>
<% end -%>
