<% @pagetitle = "Edit Raw Configuration of #{@project}"
   project_bread_crumb('Edit raw configuration')
-%>

<%= render :partial => "tabs" %>

<h3><%= @pagetitle %></h3>

<% form_tag :action => :save_meta do %>
  <p>
    <strong>Config:</strong><br/>
    <%= text_area_tag "meta", @meta, :rows => 20, :cols => 40 %><br/>
    <%= hidden_field_tag "project", @project.name %>
  </p>
  <p><%= submit_tag %></p>
<% end %>

