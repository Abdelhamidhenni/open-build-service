<% @pagetitle = "List My Requests" %>
<%
 @crumb_list = ['My Requests']
-%>

<p>
  <h2>Requests concerning me</h2>
  <%= render :partial => 'request_table', :locals => {:requests => @requests_for_me, :options => true, :by_me => false} %>
