                  <% @crumb_list.each do |link| -%>
                  <li><%= link %></li>
                  <% end -%>
                <% end -%>

            </ul>
            <!-- End page actions --> 
           </div>
          </div>
         </div>
         <div class="box_content" id="bodyContent">
         <!-- Begin content area -->
          <p class="special">

            <%= render(:partial => "layouts/flash", :object => flash) %>

            <%= @content_for_layout %>
            
          </p>
         <!-- End content area -->
