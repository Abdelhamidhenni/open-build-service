
<dl id="search_result">
<% if @search_text %>

  <% @search_what.each do |s_what| %>

  <h2><%= s_what -%>s</h2><br/>

    <% @results[s_what].send("each_#{s_what}") do |@result| %>
      <% if s_what == 'package' %>
        <%= render :partial => 'search_result_package' %>
      <% else %>
        <%= render :partial => 'search_result_project' %>
      <% end %>
    <% end %>

  <% end %>

<% end %>
</dl>

