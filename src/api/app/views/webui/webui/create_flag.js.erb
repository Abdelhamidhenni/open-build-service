<% arch = @flag.architecture ? @flag.architecture.name : nil %>
$( "#<%= @flag.fullname %>".replace(/\./g, '\\.') ).replaceWith("<%= escape_javascript(render partial: 'shared/repositories_flag_table_column', :locals => { flag: @flag, type: @flag.flag, repo: @flag.repo, architecture: arch }) %>");
$( ".spinner").hide();
